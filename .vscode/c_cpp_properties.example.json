{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}",
            "${workspaceFolder}/include"
        ],
        "myCompilerPath": "/usr/local/bin/gcc-7"
    },
    "configurations": [
        {
            "name": "Mac",
            "intelliSenseMode": "linux-gcc-x64",
            "includePath": [
                "${myDefaultIncludePath}"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks"
            ],
            "defines": [
            ],
            "forcedInclude": [
                "${workspaceFolder}/build/version-config.h"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}