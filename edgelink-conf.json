{
    "projectID": "yncic-dangerous",
    "deviceID": "3D24E79E3D7D4078B48440B528E7BF74",
    "serverUrl": "tcp://test.mosquitto.org:1883",
    "queueCapacity": 100,
    "dataflow": [
        {
            "$type": "source.inject",
            "key": "source1",
            "cron": "*/2 * * * * ?" // 每隔两秒执行一次
        },
        {
            "$type": "filter.queued",
            "key": "filter1",
            "capacity": 120
        },
        {
            "$type": "sink.logged",
            "key": "sink1"
        },
        {
            "$type": "sink.blackhole",
            "key": "sink2"
        },
        {
            "$type": "wire",
            "key": "wire1",
            "input": "source1",
            "output": "filter1"
        },
        {
            "$type": "wire",
            "key": "wire2",
            "input": "filter1",
            "output": "sink1"
        },
        {
            "$type": "wire",
            "key": "wire3",
            "input": "source1",
            "output": "sink2"
        }
    ]
}