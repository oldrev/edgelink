{
    "projectID": "yncic-dangerous",
    "deviceID": "3D24E79E3D7D4078B48440B528E7BF74",
    "serverUrl": "tcp://test.mosquitto.org:1883",
    "queueCapacity": 100,
    "dataflow": [
        {
            "$type": "source.inject",
            "key": "source1",
            "cron": "*/2 * * * * ?", // 每隔两秒执行一次
            "wires": [
                [
                    "queue1"
                ]
            ]
        },
        {
            "$type": "filter.queued",
            "key": "queue1",
            "capacity": 120,
            "wires": [
                ["sink1", "sink2"]
            ]
        },
        {
            "$type": "sink.logged",
            "key": "sink1",
            "wires": []
        },
        {
            "$type": "sink.blackhole",
            "key": "sink2",
            "wires": []
        }
    ]
}